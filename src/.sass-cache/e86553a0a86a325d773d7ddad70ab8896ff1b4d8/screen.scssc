 * In this file you should write your main styles. (or centralize your imports)
 * Import this file using the following HTML or equivalent:
 * <link href="/stylesheets/screen.css" media="screen, projection" rel="stylesheet" type="text/css" /> */

@import "compass/reset";
@import "compass/css3";
@import "variables";
@import "animations";

body, html, .shixels_section {
	width: 100%;
	height: 100%;
	//overflow: hidden;
}

h1 {
	background: $shixels_red;
	color: $shixels_white;
	font-family: $cursive;
	text-transform: capitalize;
	font-size: 4em;
	padding: 25px 0;
	margin-bottom: 45px;
	margin-top: 15px;
	text-align: center;
}

.shixels_send_button {
	
	width: 100%;
	height: 50px;
	background: $shixels_red;
	border: 0;
	color: $shixels_white;
	text-transform: uppercase;
	font-size: 1.5em;
	font-family: $sans2;
	cursor: pointer;

	&:hover {
		background: darken($shixels_red, 10%);
	}

}

.shixels_section {
	background-size: cover;
	background-position: center;
	overflow: hidden;
	position: relative;
	background-attachment: fixed;
	-webkit-transition: 2s; /* For Safari 3.1 to 6.0 */
	transition: 2s;
	.shixels_section_holder {
		width: 80%;
		margin: 0 auto;
		position: relative;
		p {
			font-size: 1em;
		}

		&.white {
			color: $shixels_white;
		}
	}
	&.noPar {
		background-attachment: scroll;
	}
	&.color {
		&.red {
			background: $shixels_red;
		}
	}

	&.branding {
		background-image: url('../img/shixels_branding2.jpg');
	}
	.shixels_bottom {

		&.branding {
			background-image: url('../img/shixels_branding.jpg');
			background-position: center;
		}
		&.web {
			background-image: url('../img/shixels_web.png');
			background-position: top;
		}
		&.devices {
			background-image: url('../img/shixels_devices.png');
			background-position: top;
			width: 80%;
			margin: 0 auto;
			position: relative;
			bottom: -100px;
		}

		&.apps {
			background-image: url('../img/shixels_app.png');
			background-position: bottom;
			margin: 0 auto;
			background-size: 50%;
		}

		&.full {
			height: 100%;
		}
		
		background-size: contain;
		background-repeat: no-repeat;
		position: absolute;
		bottom: 0;
		width: 100%;
		height: 40%;

	}
}

.shixels_slogan {
	font-weight: 100;
	text-align: center;
	position: relative;
	z-index: 1;
	top: 30%;

	.top {
		font-family: $sans;
		text-transform: uppercase;
		font-size: 3em;


		&.white {
			color: $shixels_white;
			text-transform: capitalize;
			font-size: 2em;
			font-family: $sans2;
			line-height: 3em;
		}
	}

	.bottom {
		font-family: $cursive;
		font-size: 5.5em;
		color: $shixels_red;
	}
}

.shixels_terms {
	position: relative;
	top: 25%;
	text-align: center;
	font-size: 3em;
	font-family: $sans;
	font-weight: bolder;
	text-transform: uppercase;
	color: $shixels_white;
	background: $shixels_red;
	width: 80%;
	margin: 0 auto;
	padding: 15px 0;
}
.shixels_layer {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;

    &.red { background-color: rgba(204, 0, 0, 0.5);}
    &.black { background-color: rgba(0, 0, 0, 0.5);}
    &.white { background-color: rgba(255, 255, 255, 0.5);}

    &.light {opacity: 0.5;}

}
.shixels_logo {
	position: relative;
    left: 15px;
    top: -10px;
    float: left;
    cursor: pointer;
	img {
		width: 100px;
	}
}

.shixels_nav {
	position: fixed;
	top: 0;
	width: 100%;
	height: 50px;
	background: $shixels_black;
	border-bottom: 3px solid $shixels_red;
	z-index: 2;

	.shixels_nav_options {
		float: right;
		color: white;
		font-family: $sans2;
		text-transform: uppercase;
		padding-top: 15px;

		li {
			display: inline-block;
			padding-left: 20px;
			padding-right: 20px;
			font-size: 0.8em;
			cursor: pointer;
			font-weight: 100;
			border-right: 1px solid #666;

			&.active {
				color: $shixels_red;
			}

			&:hover {
				color: $shixels_red;
			}

			&.last {
				border: none;
			}
		}
	}
}

.shixels_button {
	background: none;
	border: 3px solid $shixels_white;
	padding: 20px 50px;
	color: $shixels_white;
	text-transform: uppercase;
	font-family: "Cabin";
	font-size: 1em;
	font-weight: lighter;
	cursor: pointer;

	&.space {
		margin-right: 15px;
	}

	&:hover {
		border-color: rgba(255,255,255,0.7);
		color: rgba(255,255,255,0.7);
	}

	&:focus {
		outline: none;
	}
}
.shixels_home {
	background-image: url('../img/shixels_home.jpg');

	

	.shixels_home_buttons {
		position: relative;
		top: 35%;
		text-align: center;
	}
}

.shixels_services {
	background-image: url('../img/shixels_about.jpg');
	text-align: center;

	.shixels_services_options {
		position: relative;
		top: 15%;
		width: 80%;
		margin: 0 auto;

		.shixels_service {
			width: 200px;
			height: 200px;
			display: inline-table;
			text-align: center;

			.icon {
				background: $shixels_white;
				color: $shixels_red;
				width: 50px;
				height: 50px;
				border-radius: 100px;
				font-size: 1.5em;
				line-height: 50px;
				text-align: center;
				margin: 0 auto;
			}

			.title {
				color: $shixels_black;
				font-family: $cursive;
				padding: 10px;
				font-size: 1.5em;
			}
			.description {
				font-size: 0.8em;
				font-family: $sans2;
				color: #fff;
				font-weight: 100;
				padding-bottom: 25px;
				line-height: 1em;
			}
		}
	}
}
.shixels_about {
	background-image: url('../img/shixels_about.jpg');
	.shixels_about_holder {
		position: relative;
		top: 25%;
		text-align: center;
		width: 80%;
		margin: 0 auto;

		p {
			color: $shixels_black;
			font-family: $sans2;
			font-weight: 100;
			line-height: 1.5em;
			font-size: 1.5em;
			background: $shixels_white;
			padding: 25px;
			margin-bottom: 35px;
			margin-top: 5%;

			.strong {
				color: $shixels_red;
				font-weight: bold;
			}
		}
	}
}
.shixels_work {
	background-image: url('../img/shixels_services.jpg');
}
.shixels_services_home {
	background-image: url('../img/shixels_services.jpg');
}
.shixels_print {
	background-image: url('../img/shixels_print.jpg');
	background-attachment: scroll;

	.shixels_section_holder {
		position: relative;
		top: 20%;
	}
}

.shixels_work_holder {
	position: relative;
	top: 15%;
	width: 80%;
	margin: 0 auto;
}
.shixels_contact {
	background-image: url('../img/shixels_contact.jpg');

	.shixels_contact_form {
		position: relative;
		top: 15%;
		width: 80%;
		margin: 0 auto;

		.shixels_contact_form_element {			
			margin-right: 5%;
			width: 45%;

			input {
				width: 95%;
				height: 50px;
				margin-bottom: 15px;
				display: inline-block;
				border: 0;
				color: $shixels_red !important;
				padding: 0 15px;
				padding-right: 0;
				font-size: 1em;
				font-family: $sans2;
			}

			select {
				width: 100%;
				height: 50px;
				border: 0;
				background: #fff;
				margin-bottom: 15px;
				font-family: $sans2;
				color: $shixels_red !important;
				font-size: 1em;
			}

			textarea {
				width: 95%;
				resize: none;
				margin-bottom: 15px;
				height: 100px;
				padding: 15px;
				font-size: 1em;
				font-family: $sans2;
				color: $shixels_red !important;
			}

			&.full {
				width: 100%;
			}

			&.left {
				float: left;
			}

			&.right {
				float: right;
				margin-right: 0;
			}
		}
	}
}

.shixels_sub_section {
	position: relative;
	h2 {
		font-family: $cursive;
		color: $shixels_white;
		font-size: 3em;
		font-weight: 100;
		text-align: center;
		background: $shixels_black;
		width: 80%;
		margin: 40px auto;
		padding: 10px;
		position: relative;
		top: 25px;
	}

	h5 {
		font-family: $cursive;
		color: $shixels_red;
		font-size: 3em;
		font-weight: 100;
		text-align: center;
		background: $shixels_white;
		width: 80%;
		margin: 40px auto;
		padding: 10px;
		position: relative;
		top: 25px;
	}

	h3 {
		font-family: $cursive;
		color: $shixels_black;
		font-size: 1.5em;
		font-weight: 100;
		text-align: center;
		margin-bottom: 20px;
		margin-top: 15px;
	}

	h4 {
		font-family: $sans2;
		color: $shixels_offblack;
		font-size: 1em;
		font-weight: 100;
		font-style: italic;
		text-align: center;
		margin-bottom: 20px;
	}

	p {
		font-family: $sans2;
		line-height: 1.5em;
		font-size: 0.8em;
	}

	.shixels_col {
		float: left;
		width: 29%;
		margin: 0 15px;
		text-align: center;
	}

	.shixels_col_two {
		width: 45%;
		float: right;
		margin-right: 25px;
		text-align: center;
		background: $shixels_white;
		padding: 5px;
		min-height: 200px;
	}
}

.shixels_drop {
	display: inline-block;
	background: $shixels_white;
	color: $shixels_red;
	text-decoration: underline;
	padding: 0 25px;
	position: relative;

	i {
		cursor: pointer;
	}
}

.shixels_next_section, .shixels_prev_section {
	text-align: center;
	position: absolute;
	bottom: 15px;
	width: 100%;
	font-family:  $sans2;
	color: $shixels_white;
	text-transform: capitalize;
	font-size: 1.5em;
	cursor: pointer;

	&.white {
		color: $shixels_white !important;
	}

	&.right {
		right: 15px;
		text-align: center;
		color: $shixels_red;
		float: right;
		width: 15%;
	}

	&.left {
		left: 15px;
		text-align: center;
		color: $shixels_red;
		float: right;
		width: 15%;
	}

	 i {
		display: block;
		font-size: 2.5em;
		margin-top: 15px;
	}
}

.bottom {
	margin-bottom: 5%;
}

.work_img {
	width: 30%;
	height: auto;
	margin: 5% 10%;
	position: relative;
	cursor: pointer;
	float: left;
}

.work_img_small{
	width: 20%;
	height: auto;
	margin: 5% 15%;
	position: relative;
	float: left;

